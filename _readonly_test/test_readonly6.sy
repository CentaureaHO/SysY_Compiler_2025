// 全局变量
int ERROR_CODE = 0;  // 条件性修改
int SUCCESS_COUNT = 0;  // 会被修改
int FAILURE_COUNT = 0;  // 会被修改
int EXECUTION_TRACE[20] = {0};  // 记录执行痕迹
int trace_pos = 0;  // 痕迹位置

// 模拟操作，有可能成功或失败
int performOperation(int value) {
    // 记录执行
    EXECUTION_TRACE[trace_pos] = value;
    trace_pos = trace_pos + 1;
    
    // 偶数值成功，奇数值失败
    if (value % 2 == 0) {
        SUCCESS_COUNT = SUCCESS_COUNT + 1;
        return 1;  // 成功
    } else {
        FAILURE_COUNT = FAILURE_COUNT + 1;
        return 0;  // 失败
    }
}

// 尝试执行操作并处理错误
int tryOperation(int value) {
    int result = performOperation(value);
    
    if (result == 0) {
        // 出错时设置错误代码
        ERROR_CODE = value;
    }
    
    return result;
}

int main() {
    int total_success = 0;
    int i = 0;
    
    // 尝试各种值
    for (i = 0; i < 10; i = i + 1) {
        total_success = total_success + tryOperation(i);
    }
    
    // 输出结果
    putint(total_success);
    putint(ERROR_CODE);
    putint(SUCCESS_COUNT);
    putint(FAILURE_COUNT);
    
    return 0;
}

